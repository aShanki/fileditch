{
  "name": "fileditch",
  "version": "1.0.0",
  "description": "Temporary file storage system with customizable expiration times",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "setup": "node -e \"const fs=require('fs'); ['uploads', 'data'].forEach(dir => !fs.existsSync(dir) && fs.mkdirSync(dir));\""
  },
  "keywords": [
    "file",
    "storage",
    "temporary",
    "upload"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "express-session": "^1.17.3",
    "helmet": "^6.1.5",
    "multer": "^1.4.5-lts.1",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "nodemon": "^2.0.22"
  }
}
